{
  "name": "galileo-seo-assistant",
  "version": "0.1.0",
  "description": "A chrome extension to help you test your site SEO.",
  "scripts": {
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "test": "ava"
  },
  "ava": {
    "files": [
      "./src/**/*.specs.js"
    ],
    "source": [
      "./src/**/*.{js}",
      "!./src/**/*.specs.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-preset-env": "1.6.1",
    "clean-webpack-plugin": "0.1.17",
    "copy-webpack-plugin": "4.2.0",
    "css-loader": "0.25.0",
    "file-loader": "0.11.2",
    "fs-extra": "0.30.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.24.1",
    "style-loader": "0.13.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "write-file-webpack-plugin": "3.4.2"
  },
  "dependencies": {
    "bulma": "0.6.2",
    "font-awesome": "4.7.0",
    "xmldom": "0.1.27"
  }
}
